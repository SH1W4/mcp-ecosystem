{\n  \"bootstrap\": {\n    \"symbiotic_core_init\": {\n      \"automated\": true,\n      \"description\": \"Inicialização do núcleo simbiótico\",\n      \"triggers\": [\n        \"on_project_load\",\n        \"on_demand\"\n      ],\n      \"params\": {\n        \"host_system\": \"mcp_ecosystem\",\n        \"guest_system\": \"warp\",\n        \"symbiosis_mode\": \"full\",\n        \"core_capabilities\": [\n          \"task_management\",\n          \"document_sync\",\n          \"monitoring\",\n          \"metrics\"\n        ]\n      }\n    },\n    \"symbiotic_analysis\": {\n      \"automated\": true,\n      \"description\": \"Análise de compatibilidade simbiótica\",\n      \"dependencies\": [\"symbiotic_core_init\"],\n      \"outputs\": [\n        \"compatibility_matrix\",\n        \"adaptation_paths\",\n        \"emergence_potential\"\n      ]\n    }\n  },\n  \"integration\": {\n    \"host_bridge\": {\n      \"automated\": true,\n      \"description\": \"Ponte de integração com sistema hospedeiro\",\n      \"triggers\": [\"post_analysis\"],\n      \"capabilities\": [\n        \"resource_management\",\n        \"state_monitoring\",\n        \"event_propagation\"\n      ],\n      \"adaptation_level\": 0.85\n    },\n    \"guest_bridge\": {\n      \"automated\": true,\n      \"description\": \"Ponte de integração com sistema convidado\",\n      \"triggers\": [\"post_analysis\"],\n      \"capabilities\": [\n        \"resource_consumption\",\n        \"state_reflection\",\n        \"event_handling\"\n      ],\n      \"sync_mode\": \"realtime\"\n    }\n  },\n  \"evolution\": {\n    \"symbiotic_emergence\": {\n      \"automated\": true,\n      \"description\": \"Processo de emergência simbiótica\",\n      \"triggers\": [\n        \"on_state_change\",\n        \"scheduled\"\n      ],\n      \"metrics\": [\n        \"emergence_rate\",\n        \"symbiotic_cohesion\",\n        \"adaptation_efficiency\"\n      ]\n    },\n    \"symbiotic_evolution\": {\n      \"automated\": true